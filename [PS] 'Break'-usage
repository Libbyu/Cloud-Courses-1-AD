#break 사용법
while($true){
    $num = Read-Host "숫자를 입력해주세요."
    $a = [int]$num
    #$a = 3
    switch($a) {
        1 {
            if($a -eq 1){
                Write-Host "one"}
          }
        2 {"two"}
        3 {"three"}
        4 {"Four"}
        3 {"three again"}
        Default {break}
    }

}

$student = [PScustomObject]@{
    Name = $Name 
    Age = $Age 
}
$student.Name = 'Erik'
$student.Age = 30
